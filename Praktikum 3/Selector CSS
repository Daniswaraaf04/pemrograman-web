                        <style>
                            /*UniversalSelector*/
                            * {
                                color: brown;
                                background-color: bisque;
                            }

                            /*ElementSelector*/
                            h2 {
                                color: black;
                            }

                            /*ClassSelector*/
                            p.success {
                                color: green;
                            }

                            /*IdSelector*/
                            p#danger {
                                color: red;
                            }

                            /*AttributeSelector*/
                            a[href] {
                                font-size: 50px;
                            }

                            /*GroupSelector*/
                            h1,
                            h2,
                            p {
                                text-transform: uppercase;
                            }

                            /*ElementSpesificSelector*/
                            p.judul {
                                font-size: large;
                            }

                            span#important {
                                color: red;
                                font-weight: bold;
                            }

                            /*DescendantSelector*/
                            divp {
                                font-size: x-large;
                            }

                            /*ChildSelector*/
                            div>p {
                                color: cornflowerblue;
                            }
                        </style>
